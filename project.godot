config_version=5

[PSSFFS2KXX]

config/godotenv/root_directory="res://"

[application]

config/name="PSSFFS2KXX"
run/main_scene="res://src/Levels/LevelExample/LevelExample.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://assets/art/icons/popol.png"

[autoload]

Global="*res://src/Global/Global.gd"
GlobalEffect="*res://src/Global/GlobalEffect.gd"
GlobalMaths="*res://src/Global/GlobalMaths.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
GodotParadiseAudioManager="*res://addons/singleton_bundle/audio/audio.gd"
GodotParadiseEnvironment="*res://addons/singleton_bundle/dotenv/godotenv.gd"
GodotParadiseGeneralUtilities="*res://addons/singleton_bundle/utils/general_utilities.gd"
GodotParadiseVectorWizard="*res://addons/singleton_bundle/utils/vector_wizard.gd"
GodotParadiseSceneTransitioner="*res://addons/singleton_bundle/scene_transition/scene_transitioner.gd"

[debug]

