gdscript/warnings/unused_variable=false
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=false
gdscript/warnings/shadowed_variable=0
gdscript/warnings/return_value_discarded=false
gdscript/warnings/deprecated_keyword=2
gdscript/warnings/renamed_in_godot_4_hint=true
shapes/collision/shape_color=Color(1, 1, 1, 0.419608)
gdscript/warnings/unused_argument=false

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/energy_saving/keep_screen_on=false
window/stretch/mode="viewport"
window/ios/allow_high_refresh_rate=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/singleton_bundle/plugin.cfg")

[filesystem]

import/blender/enabled=false

[importer_defaults]

texture={
"detect_3d": false,
"flags/filter": false
}
scene={
